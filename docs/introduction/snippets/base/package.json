{
  "name": "@tsed/basic-example",
  "version": "1.0.0",
  "description": "Here an example to configure your server and how to create your first controller.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "7.61.0",
    "@tsed/common": "7.61.0",
    "@tsed/core": "7.61.0",
    "@tsed/di": "7.61.0",
    "@tsed/exceptions": "7.61.0",
    "@tsed/schema": "7.61.0",
    "@tsed/json-mapper": "7.61.0",
    "@tsed/platform-express": "7.61.0",
    "@tsed/swagger": "7.61.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "compression": "1.7.4",
    "concurrently": "5.3.0",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "method-override": "^3.0.0",
    "uuid": "8.3.0",
    "cross-env": "7.0.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.7",
    "@types/http-proxy": "1.17.2",
    "@types/mocha": "8.0.3",
    "@types/node": "14.11.1",
    "@types/request-promise": "4.1.45",
    "@types/supertest": "6.0.2",
    "concurrently": "5.3.0",
    "nodemon": "1.19.4",
    "rimraf": "3.0.0",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.9.5"
  }
}
